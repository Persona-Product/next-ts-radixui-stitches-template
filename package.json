{
  "name": "ts-fullstack-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "fix:lint": "next lint --fix",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --write .",
    "prepare": "husky install",
    "test": "jest --watch",
    "test-total-coverage": "jest --collectCoverage=true --coverageReporters=json-summary",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.7",
    "@radix-ui/react-alert-dialog": "^0.1.0",
    "@radix-ui/react-hover-card": "^0.1.0",
    "@radix-ui/react-icons": "^1.0.3",
    "@radix-ui/react-id": "^0.1.0",
    "@radix-ui/react-popover": "^0.1.0",
    "@radix-ui/react-radio-group": "^0.1.0",
    "@radix-ui/react-switch": "^0.1.0",
    "@stitches/react": "^1.2.1",
    "next": "12.1.0",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import-access": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
